		var bid_slots = window.simplyrecipes.ads.amazon.bidding.bid_slots;
		log.write('Fetching Amazon bids for ' + bid_slots.length + ' slots', true);
		apstag.fetchBids({slots: bid_slots}, function(bids) {
			googletag.cmd.push(function() {
				log.write('Fetched ' + (bids ? bids.length : 'no') + ' Amazon bids', true);
				apstag.setDisplayBids();
				var refresh_slots = [];
				bids.forEach(function(bid) {
					refresh_slots.push(slots[bid.slotID]);
				});
				if (refresh_slots.length > 0) {
					googletag.pubads().refresh(refresh_slots);
				}
			});
		});
	});
}
