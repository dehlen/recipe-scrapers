		if (target_params) {
			var targets = decodeURIComponent(target_params[2]);
			log.write('Targeting params detected: ' + targets, true);
			targets.split(',').forEach(function(pair) {
				var target = pair.split(':');
				if (target.length == 2) {
					log.write('Adding targeting param: "' + target[0].replace("'", "\'") + '"="' + target[1].replace("'", "\'") + '"', true);
