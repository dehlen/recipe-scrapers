<script>
    window.GujAd = window.GujAd || {};
    GujAd.cmd = GujAd.cmd || [];

    if (window.Element && !Element.prototype.closest) {
        Element.prototype.closest = function(s) {
            var matches = (this.document || this.ownerDocument).querySelectorAll(s),
                    i,
                    el = this;
            do {
                i = matches.length;
                while (--i >= 0 && matches.item(i) !== el) {};
            } while ((i < 0) && (el = el.parentElement));

            return el;
        };
    }

    (function () {
        var deferredElement = document.querySelectorAll('#recipe-image-carousel .recipe-image-carousel-slide');
        var lighbox = document.querySelector('#recipe-image-lightbox');
        var lighboxRecipeSlider = document.querySelector('#recipe-image-lightbox-carousel');
        var ads = {
            initialized: false,
            slideChangeCount: 0,
            reload: function () {
                ads.slideChangeCount = 0;
                var event = document.createEvent('Event');
                event.initEvent('ck.reloadAdSlots', true, true);
                lighbox.dispatchEvent(event);
            }
        }

        var ivw = {
            createIframe: function() {
                var config = JSON.parse((document.querySelector('#infonline')).textContent);
                var iFrame = document.createElement('iframe');
                var src = config.requests.url;

                var params = Object.assign({
                    sv: 'ke',
                    ap: 1,
                    type: 'event',
                    ev: '',
                    host: '${host}',
                    path: '${path}'
                }, config.extraUrlParams);

                Object.keys(params).forEach(function(key, index) {
                    src += (index === 0 ? '?' : '&') + key + '=' + params[key];
                });

                var screenWidth = window.screen.width;
                src = src.replace('${screenWidth}', screenWidth);
                src = src.replace('${host}', window.location.hostname);
                src = src.replace('${path}', window.location.pathname);
                src = src.replace('RANDOM', Math.random());

                iFrame.src = src;

                return iFrame;
            },

            sendRequest: function() {
                var container = document.querySelector('#ivw-iframe-container');
                container.innerHTML = '';

                var iFrame = ivw.createIframe();
                container.appendChild(iFrame);
            }
        };

        Array.prototype.map.call(deferredElement, function(element) {
            element.addEventListener('click', function (event) {
                // Set to -1, because the opening is the first slide change..
                ads.slideChangeCount = -1;
                if (!ads.initialized) {
                    ads.initialized = true;
                } else {
                    ads.reload();
                }
                ivw.sendRequest();
            });
        });

        lighboxRecipeSlider.addEventListener('slideChange', function () {
            if (ads.slideChangeCount < 2) {
                ads.slideChangeCount++;
                return;
            }
            ads.reload();
            ivw.sendRequest();
        });
    })();
</script>

